{
    "name": "esign/laravel-plytix",
    "description": "A Laravel SDK for the Plytix API",
    "keywords": [
        "esign",
        "plytix"
    ],
    "homepage": "https://github.com/esign/laravel-plytix",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jordy Van der Haegen",
            "email": "jordy.vanderhaegen@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.5",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Esign\\Plytix\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Esign\\Plytix\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "phpcs": "vendor/bin/php-cs-fixer fix"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Esign\\Plytix\\PlytixServiceProvider"
            ],
            "aliases": {
                "Plytix": "Esign\\Plytix\\Facades\\PlytixFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
